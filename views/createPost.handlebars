<div>
  <h2>Create a Post</h2>
  <form class="postForm">
    <div>
      <label for="title">Title:</label>
      <input type="text" id="title" placeholder="Enter a title of your choice">
     </input>
    </div>
    <div>
      <label for="content">Content:</label>
      <textarea id="content" rows="3"  placeholder="title"
      ></textarea>
    </div>
      <button type="submit">Upload Post</button>
  </form>
</div>
